                l20[n]=a[i]
                n+=1
    return l1,l2,l3,l4,l5,l6,l7,l8,l9,l10,l11,l12,l13,l14,l15,l16,l17,l18,l19,l20
import time
def countdown(n):
    while(countdown!=0):#proper working ke liye jb tk cou nter chlta h
        ques()
        f=input("enter 1 for accross and 2 for down")#choice h 
        if(f=="1"):
            accross()
            show()
        if(f=="2"):
            down()
            show()
        else:
            print("wrong choice")    #defaultn  agr error AYI TOH
#values compare krke marks show krn h ab or ans show krde
countdown(2)
ans()
