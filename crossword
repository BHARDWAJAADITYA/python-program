#accross
def accross():
    print("choose a ques no.\n1,3,4,8,9,13,16,18,19,20")
    q=int(input("enter the q no."))
    if(q==1):
        a=[]
        b=input("enter the STRING")
        a=list(b)
        if(len(a)==7):
            n=7
            for i in range(7):
                l1[n]=a[i]
                n+=1
    if(q==3):
        a=[]
        b=input("enter the STRING")
        a=list(b)
        if(len(a)==16):
            n=2
            for i in range(16):
                l3[n]=a[i]
                n+=1
    if(q==4):
        a=[]
        b=input("enter the STRING")
        a=list(b)
        if(len(a)==5):
            n=8
            for i in range(7):
                l5[n]=a[i]
                n+=1
    if(q==8):
        a=[]
        b=input("enter the STRING")
        a=list(b)
        if(len(a)==8):
            n=2
            for i in range(7):
                l8[n]=a[i]
                n+=1
    if(q==9):
        a=[]
        b=input("enter the STRING")
        a=list(b)
        if(len(a)==6):
            n=12
            for i in range(7):
                l8[n]=a[i]
                n+=1
    if(q==13):
        a=[]
