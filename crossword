        a=list(b)
        if(len(a)==8):
            n=7
            for i in range(7):
                l12[n]=a[i]
                n+=1
    if(q==16):
        a=[]
        b=input("enter the STRING")
        a=list(b)
        if(len(a)==5):
            n=5
            for i in range(7):
                l14[n]=a[i]
                n+=1
    if(q==18):
        a=[]
        b=input("enter the STRING")
        a=list(b)
        if(len(a)==7):
            n=0
            for i in range(7):
                l17[n]=a[i]
                n+=1
    if(q==19):
        a=[]
        b=input("enter the STRING")
        a=list(b)
        if(len(a)==12):
            n=8
            for i in range(7):
                l17[n]=a[i]
                n+=1
    if(q==20):
        a=[]
        b=input("enter the STRING")
        a=list(b)
        if(len(a)==10):
            n=5
